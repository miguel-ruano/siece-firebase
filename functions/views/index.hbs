  <script type="text/javascript">
    document.addEventListener("DOMContentLoaded", () => {
      {{#if investment_by_country}}
      let chart = new Morris.Bar({
        element: 'chart',
        data: [
          {{#each investment_by_country}}
            {
              'country': '{{this.country_name}}',
              'amount': '{{this.amount}}'
            },
          {{/each}}
        ],
        xkey: 'country',
        ykeys: ['amount'],
        labels: ['Inversión'],
        barColors:["#26B99A","#34495E","#ACADAC","#3498DB"],
        hideHover:"auto",
        resize: true,
        xLabelMargin: 10
      });
      {{/if}}

      {{#if error}}
        console.error(error);
      {{/if}}
    });
  </script>

