	<script type="text/javascript">
    function changePanelsQuantity(value) {
      // console.log(value);
      let container = $("#programs_container");

      let current_quantity = container.children().length;
      // console.log(current_quantity);
      let target_quantity = value - current_quantity;
      if (target_quantity > 0) {
        for (let i = current_quantity + 1; i <= value; i++) {
          var html = `
            <div class="x_panel">
              <div class="x_title">
                <h2>Programa ${i} </h2>
                <div class="nav navbar-right">
                  <a class="collapse-link pointer">
                    <i class="fa fa-chevron-up"></i>
                  </a>
                </div>
                <div class="clearfix"></div>
              </div>
              <div class="x_content">
                <div class="form-group">
                  <label class="control-label col-md-3 col-sm-3 col-xs-12">
                    Nivel
                  </label>
                  <div class="col-md-9 col-sm-9 col-xs-12">
                    <div class="radio">
                      <label>
                        <input class="level" type="radio" value="pregrado" id="${'program'+i+'_level_pregrado'}" name="${'program'+i+'_level'}" checked required>
                        Pregrado
                      </label>
                    </div>
                    <div class="radio">
                      <label>
                        <input class="level" type="radio" value="posgrado" id="${'program'+i+'_level_posgrado'}" name="${'program'+i+'_level'}">
                        Posgrado
                      </label>
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <label class="control-label col-md-3 col-sm-3 col-xs-12" for="${'program'+i+'_name'}">
