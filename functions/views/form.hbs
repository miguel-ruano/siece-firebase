    function sendForReview() {
      console.log($("#complete").attr('value'));
      if ($("#complete").attr('value')==1){
        $("#accept").attr('formnovalidate','formnovalidate');
        $("#demo-form2").submit();
        console.log("second enviar");
      }
      $('<input />').attr('type', 'hidden')
          .attr('id', "complete")
          .attr('name', "complete")
          .attr('value', 1)
          .appendTo('#demo-form2');
      $("#demo-form2").validate({
        submitHandler: function(form) {
          console.log($("#complete").attr('value'));
          form.submit();
        }
      });
    }

    function validateTotalCurrentPortfolio() {
      let total1 = Number($("#current_portfolio_pregrado_pais_amount").val()) +
              Number($("#current_portfolio_pregrado_exterior_amount").val()) +
              Number($("#current_portfolio_posgrado_pais_amount").val()) +
              Number($("#current_portfolio_posgrado_exterior_amount").val());
      let total2 = Number($("#current_portfolio_pregrado_female").val()) +
              Number($("#current_portfolio_pregrado_male").val()) +
              Number($("#current_portfolio_posgrado_female").val()) +
              Number($("#current_portfolio_posgrado_male").val());

      //console.log(total1 + " - " + total2);
      return total1 === total2;
    }

    window.Parsley.addValidator('currentPortfolio', {
      validateNumber: function() {
        return validateTotalCurrentPortfolio();
      },
      messages: {
        es: 'Los totales de las preguntas 7 y 7A deben coincidir'
      }
    });

    function validateTotalPastduePortfolio() {
      let total1 = Number($("#pastdue_portfolio_pregrado_pais_amount").val()) +
