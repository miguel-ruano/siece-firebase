              Number($("#pastdue_portfolio_pregrado_male").val()) +
              Number($("#pastdue_portfolio_posgrado_female").val()) +
              Number($("#pastdue_portfolio_posgrado_male").val());

      //console.log(total1 + " - " + total2);
      return total1 === total2;
    }

    window.Parsley.addValidator('pastduePortfolio', {
